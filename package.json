{
  "name": "p5js-notebook",
  "displayName": "p5.js Notebook",
  "description": "Notebook for p5.js",
  "version": "0.1.0",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "keywords": [
    "notebookRenderer",
    "p5.js",
    "visual programming",
    "creative coding",
    "teaching",
    "notebook",
    "education"
  ],
  "categories": [
    "Education",
    "Notebooks",
    "Visualization",
    "Other"
  ],
  "activationEvents": [
    "onNotebook:p5notebook"
  ],
  "main": "./out/extension/extension.js",
  "browser": "./out/extension/extension.web.js",
  "contributes": {
    "notebooks": [
      {
        "type": "p5notebook",
        "displayName": "p5.js Notebook",
        "priority": "default",
        "selector": [
          {
            "filenamePattern": "*.p5js"
          }
        ]
      }
    ],
    "notebookRenderer": [
      {
        "id": "io.garrit.p5notebook",
        "displayName": "p5.js",
        "mimeTypes": [
          "text/p5js"
        ],
        "entrypoint": "./out/client/index.js"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production",
    "lint": "eslint src --ext ts",
    "watch": "webpack --mode development --watch",
    "pretest": "webpack --mode development && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/jshint": "^2.12.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/vscode": "^1.67.0",
    "@types/vscode-notebook-renderer": "^1.60.0",
    "@types/webpack-env": "^1.16.4",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vscode/test-electron": "^2.1.3",
    "assert": "^2.0.0",
    "css-loader": "^4.2.0",
    "eslint": "^8.14.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "glob": "^8.0.1",
    "jshint": "^2.13.5",
    "mocha": "^9.2.2",
    "raw-loader": "^4.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.4",
    "util": "^0.12.4",
    "vscode-notebook-error-overlay": "^1.0.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixelkind/p5notebook"
  },
  "bugs": {
    "url": "https://github.com/pixelkind/p5notebook/issues"
  },
  "contributors": [
    {
      "name": "Garrit Schaap",
      "url": "https://github.com/pixelkind"
    }
  ],
  "author": {
    "name": "Garrit Schaap",
    "url": "https://github.com/pixelkind"
  }
}
